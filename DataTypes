
public class DataTypes {

	public static void main(String[] args) {
	char c= 'a';
	System.out.println(c + 3);
	
	int i= c+10;
	System.out.println(i);
	
	//char abc=i;
	
	long l=i;
	//i = l;
	//int can be copies in long but not vice versa
	//this will bw true in each case

	double d = i;
	//i=d;
	
	float f = 1.23f;
	//f = d;
	d = f;
	
	
	int j =100;
	}

}
